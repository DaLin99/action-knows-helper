<view><image class="img" src="{{itemDetail.imgPath}}"></image><uni-list vue-id="09fb963e-1" bind:__l="__l" vue-slots="{{['default']}}"><uni-list-item vue-id="{{('09fb963e-2')+','+('09fb963e-1')}}" title="失物名称" rightText="{{itemDetail.title}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('09fb963e-3')+','+('09fb963e-1')}}" title="丢失时间" rightText="{{itemDetail.time}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('09fb963e-4')+','+('09fb963e-1')}}" title="丢失地点" rightText="{{itemDetail.place}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('09fb963e-5')+','+('09fb963e-1')}}" title="发布时间" rightText="{{itemDetail.publishDate}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('09fb963e-6')+','+('09fb963e-1')}}" title="拾到者" rightText="{{itemDetail.publisher}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('09fb963e-7')+','+('09fb963e-1')}}" title="联系微信" rightText="{{itemDetail.wechat}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('09fb963e-8')+','+('09fb963e-1')}}" title="电话" rightText="{{itemDetail.tell}}" bind:__l="__l"></uni-list-item></uni-list><view class="content"><text>{{itemDetail.content}}</text></view><block wx:if="{{userInfo.openid===itemDetail.publisherId}}"><button class="mt40" type="primary" data-event-opts="{{[['tap',[['edit',['$event']]]]]}}" bindtap="__e">修改</button></block><block wx:else><block wx:if="{{itemDetail.favorite===0}}"><button class="mt40" type="primary" data-event-opts="{{[['tap',[['addFavorite',['$event']]]]]}}" bindtap="__e">收藏</button></block><block wx:else><button class="mt40" type="primary" data-event-opts="{{[['tap',[['cancelFavorite',['$event']]]]]}}" bindtap="__e">取消收藏</button></block></block></view>